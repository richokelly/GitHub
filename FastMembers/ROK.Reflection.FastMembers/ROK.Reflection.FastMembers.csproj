<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Authors>rich.okelly</Authors>
    <Company>rich.okelly</Company>
    <Description>Simplifies member (field and property) access via an intuitive (and highly performant) interface.</Description>
    <Title>Fast Members</Title>
    <PackageTags>C#;Reflection;Member Access;</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <IncludeSymbols>True</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <PackageId>$(AssemblyName)Core</PackageId>
  </PropertyGroup>

</Project>
